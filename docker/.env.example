
# Telegram bot token (get from @BotFather)
TELEGRAM_TOKEN=your_telegram_bot_token_here

# RabbitMQ configuration
RABBITMQ_URL=amqp://user:password@rabbitmq:5672/

# MinIO configuration
MINIO_ENDPOINT=minio:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_USE_SSL=false

# OpenRouter API configuration
# These variables configure access to the OpenRouter API
OPENROUTER_API_KEY=openrouter-api-key
OPENROUTER_MODEL=gpt-4o
OPENROUTER_MAX_TOKENS=300
OPENROUTER_TEMPERATURE=0.7
OPENROUTER_PROMPT="Requires descriptions and keywords in English for each uploaded image for stock photo submissions. Descriptions should be concise (up to 200 characters) with the most relevant keywords. Keywords list should include 49 words."
USE_OPENROUTERGO_ADAPTER=false

# Server configuration
SERVER_PORT=8080